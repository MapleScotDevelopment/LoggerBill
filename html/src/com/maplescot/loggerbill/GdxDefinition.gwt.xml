<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
	<inherits name='com.badlogic.gdx.backends.gdx_backends_gwt' />

	<inherits name='LoggerBillGame' />
	<entry-point class='com.maplescot.loggerbill.client.HtmlLauncher' />
	
	<set-configuration-property name="gdx.assetpath" value="../android/assets" />
    <extend-configuration-property name="gdx.reflect.include" value="com.maplescot.loggerbill" />
    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.scenes" />
</module>
